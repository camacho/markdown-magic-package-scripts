{
  "name": "markdown-magic-package-scripts",
  "version": "1.2.2",
  "description": "Print list of scripts in package.json with descriptions",
  "license": "MIT",
  "repository": "git@github.com:camacho/markdown-magic-package-scripts.git",
  "author": "Patrick Camacho <patrick@daftdevelopers.com>",
  "main": "index.js",
  "keywords": [
    "markdown",
    "package",
    "scripts",
    "package-scripts",
    "markdown-magic",
    "npm"
  ],
  "scripts": {
    "docs": "node example.js",
    "empty": "echo \"this is just an example\"",
    "format": "prettier --single-quote --trailing-comma es5 --write \"*.js\"",
    "prepublish": "npm run docs"
  },
  "dependencies": {
    "findup": "^0.1.5",
    "sort-scripts": "^1.0.1"
  },
  "peerDependencies": {
    "markdown-magic": ">=0.1 <=2.x"
  },
  "devDependencies": {
    "markdown-magic": "^2.0.0",
    "markdown-magic-install-command": "^1.3.1",
    "prettier": "^2.0.0"
  }
}
